{
  "name": "my-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:check": "gts lint",
    "lint:fix": "gts fix"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@reduxjs/toolkit": "^1.8.6",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "next": "13.1.1",
    "node-polyglot": "^2.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-polyglot": "^0.7.2",
    "react-redux": "^8.0.4",
    "typescript": "4.4.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.24.0",
    "eslint-config-next": "13.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "gts": "^3.1.1",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4"
  },
  "prettier": "gts/.prettierrc.json",
  "eslintConfig": {
    "extends": "./node_modules/gts/",
    "plugins": [
      "simple-import-sort"
    ],
    "rules": {
      "node/no-unpublished-import": 0,
      "simple-import-sort/exports": "error",
      "simple-import-sort/imports": "error",
      "@typescript-eslint/no-unused-vars": [
        "error"
      ],
      "max-classes-per-file": [
        "error",
        1
      ]
    },
    "ignorePatterns": [
      "node_modules/",
      "coverage*/",
      "build/",
      ".next/"
    ]
  }
}
